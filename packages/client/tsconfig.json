{
	"compilerOptions": {
		"module": "esnext",
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"esModuleInterop": true,
		"strict": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx"
	},
	"include": ["src"],
	"extends": "../../tsconfig.base.json"
}
